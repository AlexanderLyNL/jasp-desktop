{
	"name": "BainRegressionLinearBayesian",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "model",
			"type": "String"
		},
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"name": "effectsStepwise",
			"type": "Boolean"
		},
		{
			"name": "posteriorEstimates",
			"type": "Boolean"
		},
		{
			"name": "posteriorDistributions",
			"type": "Boolean"
		},
		{
            "options": [ "BF01", "LogBF10" ],
            "default": "BF01",
			"name": "bayesFactorType",
			"type": "List"
		},
		{
			"name": "posteriorEstimates",
			"type": "Boolean"
		},
		{
			"name": "posteriorEstimatesCredibleIntervalInterval",
			"type": "Number",
			"format": "%",
			"value": 0.95,
			"min": 0.5,
			"max": 0.999
		},
		{
			"name": "posteriorEstimatesMCMCIterations",
			"type": "Number",
			"value": 10000,
			"min": 1,
			"max": 1000000
		},
		{
			"name": "priorCovariates",
			"type": "Number",
			"value": 0.354,
			"min": 0,
			"max": 2
		}
	]
}
